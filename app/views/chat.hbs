<h2>Chat!</h2>

<div id="chat-container">
    <div id="chat-messages">
        {{#each messages}}
            {{> chat/message message=this}}
        {{/each}}
    </div>
    <div id="chat-input">
        <input type="text" id="message-input" placeholder="Message...">
        <button id="send-button">Send</button>
    </div>
</div>